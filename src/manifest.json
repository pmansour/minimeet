{
    "name": "MiniMeet",
    "version": "1.0.7",
    "description": "Automatically join meetings on your calendar.",
    "permissions": [
        "https://meet.google.com/"
    ],
    "background": {
        "scripts": ["scripts/background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_title": "Join the next meeting on your calendar."
    },
    "manifest_version": 2,
    "web_accessible_resources": [
        "scripts/findMeeting.js",
        "scripts/joinMeeting.js"
    ]
}